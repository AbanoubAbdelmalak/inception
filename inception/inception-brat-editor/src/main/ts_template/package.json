{
  "name": "@inception-project/${project.artifactId}",
  "version": "${semver}",
  "author": "INCEpTION Team",
  "license": "(MIT AND Apache-2.0)",
  "scripts": {
    "build": "node build.mjs",
    "test": "cross-env JUNIT_REPORT_PATH=../../../target/surefire-reports/mocha-test-report.xml mocha -r esbuild-runner-plugins/register './test/**/*.test.ts' --extension ts --reporter mocha-jenkins-reporter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inception-project/inception.git"
  },
  "bugs": {
