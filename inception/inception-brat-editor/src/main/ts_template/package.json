    "mocha": "${mocha.version}",
    "mocha-jenkins-reporter": "${mocha-jenkins-reporter.version}",
    "typescript": "${typescript.version}",
    "sass": "${sass.version}",
    "@typescript-eslint/eslint-plugin": "${typescript-eslint.version}",
    "@typescript-eslint/parser": "${typescript-eslint.version}",
    "@types/chai": "${types-chai.version}",
    "@types/events": "${types-events.version}",
    "@types/jquery": "${types-jquery.version}",
    "@types/mocha": "${types-mocha.version}",
    "@types/urijs": "${types-urijs.version}",
    "uuid": "${uuid.version}",
    "yargs": "${yargs.version}"
  }
