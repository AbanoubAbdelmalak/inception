{
  "name": "pdfanno",
  "version": "0.5.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:pdfanno.core": "esbuild src/core/index.js --target=es6 --bundle --sourcemap --global-name=pdfanno.core --outfile=dist/pdfanno/pdfanno.core.bundle.js",
    "build:pdfanno.page": "esbuild src/pdfanno.js --target=es6 --bundle --sourcemap --global-name=pdfanno.page --outfile=dist/pdfanno/pdfanno.page.bundle.js",
    "build:viewer": "esbuild src/viewer.js --target=es6 --bundle --sourcemap --global-name=viewer --external:'images/loading-icon.gif' --external:'images/*' --outfile=dist/pdfanno/viewer.bundle.js",
    "build:compatibility": "esbuild src/compatibility.js --target=es6 --bundle --sourcemap --global-name=compatibility --external:'images/loading-icon.gif' --external:'images/*' --outfile=dist/pdfanno/compatibility.bundle.js",
    "build:l10n": "esbuild src/l10n.js --target=es6 --bundle --sourcemap --global-name=l10n --external:'images/loading-icon.gif' --external:'images/*' --outfile=dist/pdfanno/l10n.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paperai/pdfanno"
  },
  "author": "hshindo, yoheiMune",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paperai/pdfanno/issues"
  },
  "homepage": "https://github.com/paperai/pdfanno#readme",
  "pdfextract": {
    "version": "0.3.0",
    "url": "https://github.com/paperai/pdfextract/releases/download/v0.3.0/pdfextract-0.3.0.jar"
  },
  "devDependencies": {
    "esbuild": "^0.13.12",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "anno-ui": "github:paperai/anno-ui#master",
    "gulp-htmlmin": "^4.0.0",
    "pako": "^1.0.6",
    "events": "^3.3.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.2",
    "requirejs": "^2.3.5",
    "semver": "^5.5.0",
    "toml": "github:yoheiMune/toml-node",
    "urijs": "^1.19.0"
  }
}
